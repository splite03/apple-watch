g(e);\n      }\n    }\n  },\n  mounted() {\n    const a = document.querySelectorAll('a');\n    a.forEach(e => e.addEventListener('click', event => event.preventDefault()));\n  }\n};","map":{"version":3,"mappings":"AAwSA,OAAOA,MAAK,MAAO,0BAAyB;AAC5C,OAAOC,eAAc,MAAO,oCAAmC;AAC/D,OAAOC,sBAAqB,MAAO,4CAA2C;AAC9E,OAAOC,QAAO,MAAO,4BAA2B;AAChD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IAACL,MAAM;IAACC,eAAe;IAAEC,sBAAsB;IAAEC;EAAQ,CAAC;EACtEG,QAAQ,EAAC;IACPC,MAAM,GAAE;MACN,OAAOC,QAAQ,CAACC,aAAa,CAAC,sBAAsB;IACtD,CAAC;IACDC,cAAc,GAAE;MACd,OAAOF,QAAQ,CAACG,gBAAgB,CAAC,sBAAsB,CAAC,CAAC,EAAE;IAC7D,CAAC;IACDC,cAAc,GAAE;MACd,OAAOJ,QAAQ,CAACG,gBAAgB,CAAC,sBAAsB,CAAC,CAAC,CAAC;IAC5D,CAAC;IACDE,gBAAgB,GAAG;MACjB,OAAOL,QAAQ,CAACG,gBAAgB,CAAC,uBAAuB;IAC1D;EACF,CAAC;EACDG,OAAO,EAAC;IACNC,KAAK,CAACC,IAAI,EAAC;MACT,IAAG;QACD,QAAOA,IAAI;UACT,KAAK,IAAI;YACPC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACR,cAAc,CAAC;YAChC,IAAI,CAACA,cAAc,CAACS,SAAS,CAACC,MAAM,CAAC,6BAA6B;YAClE,IAAI,CAACb,MAAM,CAACY,SAAS,CAACC,MAAM,CAAC,iCAAiC;YAC9D,IAAI,CAACP,gBAAgB,CAAC,CAAC,CAAC,CAACM,SAAS,CAACC,MAAM,CAAC,aAAa;YACvD,IAAI,CAACP,gBAAgB,CAAC,CAAC,CAAC,CAACM,SAAS,CAACC,MAAM,CAAC,aAAa;YAEvD,IAAI,CAACR,cAAc,CAACO,SAAS,CAACE,GAAG,CAAC,6BAA6B;YAC/D,IAAI,CAACd,MAAM,CAACY,SAAS,CAACE,GAAG,CAAC,iCAAiC;YAC3D;UACF,KAAK,IAAI;YACP,IAAI,CAACT,cAAc,CAACO,SAAS,CAACC,MAAM,CAAC,6BAA6B;YAClE,IAAI,CAACb,MAAM,CAACY,SAAS,CAACC,MAAM,CAAC,iCAAiC;YAE9D,IAAI,CAACV,cAAc,CAACS,SAAS,CAACE,GAAG,CAAC,6BAA6B;YAC/D,IAAI,CAACd,MAAM,CAACY,SAAS,CAACE,GAAG,CAAC,iCAAiC;YAC3D,IAAI,CAACR,gBAAgB,CAAC,CAAC,CAAC,CAACM,SAAS,CAACE,GAAG,CAAC,aAAa;YACpD,IAAI,CAACR,gBAAgB,CAAC,CAAC,CAAC,CAACM,SAAS,CAACE,GAAG,CAAC,aAAa;YACpDJ,OAAO,CAACC,GAAG,CAAC,IAAI,CAAC;YACjB;QAAI;MAEV,CAAC,QAAMI,CAAC,EAAC;QACPL,OAAO,CAACC,GAAG,CAACI,CAAC,CAAC;MAChB;IACF;EACF,CAAC;EACDC,OAAO,GAAE;IACP,MAAMC,IAAIhB,QAAQ,CAACG,gBAAgB,CAAC,GAAG;IACvCa,CAAC,CAACC,OAAO,CAACH,KAAKA,CAAC,CAACI,gBAAgB,CAAC,OAAO,EAAEC,KAAK,IAAKA,KAAK,CAACC,cAAc,EAAE,CAAC;EAC9E;AACF","names":["navBar","appleWithHeader","compareIt